<%- include ("adminheader.ejs") %>

<%- include ("adminnav.ejs") %>

            <!-- About Start -->
            <div class="about wow fadeInUp" data-wow-delay="0.1s">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-12 col-md-12">
<div class="about-text">
<h2>Add Sub Category Here!!!</h2>
<font style="color:blue;"><%- output %></font>
<form method="post" action="/admin/addsubcategory" enctype="multipart/form-data">
    <div class="form-group">
      <label for="catname">Category Name:</label>
      <select class="form-control" id="catname" name="catname">
        <option>Select Category</option>
        <% for(let row of clist) { %>
          <option><%- row.catname %></option>
        <% } %>
      </select>
    </div>
    <div class="form-group">
      <label for="subcatname">Sub Category Name:</label>
      <input type="text" class="form-control" id="subcatname" name="subcatname">
    </div>
    <div class="form-group">
      <label for="caticon">Sub Category Icon:</label>
      <input type="file" class="form-control" id="caticon" name="caticon"> 
    </div>
    <button type="submit" class="btn btn-success">Add Sub Category</button>
  </form>    
</div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- About End -->

<%- include ("adminfooter.ejs") %>            


